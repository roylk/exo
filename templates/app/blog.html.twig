{% extends 'base.html.twig' %}

{% block title %}blog-{{parent()}}{% endblock %}

{% block body %}
  <div class="container">  
    <h1>Blog </h1>
        <div class ="grid">
          {% for article in articles %}
            <div class="card">
              <h2> {{ article.title }}</h2>
              <p> {{ article.content }}</p>
            </div>
          {% endfor %}
        </div>
        
  </div>
{% endblock %}

